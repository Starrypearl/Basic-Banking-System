
    <div class="container">
        <table class="table table-hover">
            <thead>
                <tr>
                
                  <th scope="col">Username</th>
                  <th scope="col">email</th>
                  <th scope="col">Contact</th>
                  <th scope="col">Account Balance </th>
                  <th scope="col">Transfer history </th>
                  <th scope="col">Transfer
                    Money </th>
                </tr>
              </thead>
              <tbody>
                <% for(let c of customer) {%>
                <tr>
                    <th scope="col"><%= c.username %></th>
                    <th scope="col"><%= c.email %></th>
                    <th scope="col"><%= c.contact %></th>
                    <th scope="col">$<%= c.Balance %></th>
                    <th scope="col"> <a class="btn btn-outline-secondary" href="/customer/<%= c._id %>/history"> history</a> </th>
                    <th scope="col"><a class="btn btn-outline-primary mt-1 mt-md-0" href="/customer/<%= c._id %>/transfer">Transfer
                        Money</a> </th>
                </tr>
                <% } %>
              </tbody>
      </table>
    </div>